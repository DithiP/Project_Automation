<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="cross Browser Testing">
	<listeners>
		<listener class-name="generatetestScript.Listeners" />
	</listeners>
	<test name="chrome test">
		<parameter name="Browser" value="Chrome"></parameter>
		<parameter name="userName" value="dithi12"></parameter>
		<parameter name="password" value="admin"></parameter>
		<classes>
			<class name="testScript.LoginPage_Test">
				<methods>
					<include
						name="verify_User_is_Able_To_Login_Using_Valid_Credentials">
					</include>
				</methods>
			</class>
		</classes>
	</test> <!--
	TestNGFile -->
	<test name="edge test">
		<parameter name="Browser" value="Edge"></parameter>
		<parameter name="userName" value="admin"></parameter>
		<parameter name="password" value="admin"></parameter>
		<classes>
			<class name="testScript.LoginPage_Test">
				<methods>
					<include
						name="verify_User_is_Able_To_Login_Using_Valid_Credentials">
					</include>
				</methods>
			</class>
		</classes>
	</test> <!--
	TestNGFile -->
	<test name="firefox test">
	<parameter name="Browser" value="firefox"></parameter>
	<parameter name="userName" value="admin"></parameter>
	<parameter name="password" value="admin"></parameter>
	<classes>
		<class name="testScript.LoginPage_Test">
			<methods>
				<include
					name="verify_User_is_Able_To_Login_Using_Valid_Credentials">
				</include>
			</methods>
		</class>
	</classes>
</test> <!--
	TestNGFile -->

